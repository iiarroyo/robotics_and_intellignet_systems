<?xml version="1.0" encoding="UTF-8"?>
<launch>

 <!-- these are the arguments you can pass this launch file, for example paused:=true -->
  <arg name="paused" default="false"/>
  <arg name="use_sim_time" default="true"/>
  <arg name="gui" default="true"/>
  <arg name="headless" default="false"/>
  <arg name="debug" default="false"/>

  <node name="" pkg="puzzlebot_sim" type="transform.py" output="screen"/>
  <node name="localization_variant" pkg="puzzlebot_sim" type="localization_variant.py" output="screen"/>
  <include file="$(find puzzlebot_rviz_ch1)/launch/puzzlebot_joints.launch"/>




</launch>